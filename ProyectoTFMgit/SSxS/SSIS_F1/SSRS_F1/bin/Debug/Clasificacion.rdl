  <DataSets>
    <DataSet Name="Clasif">
      <Query>
        <DataSourceName>DWH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Temporadas">
            <Value>=Parameters!Temporadas.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Circuito">
            <Value>=Parameters!Circuito.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT      Dp.driverRef, Fc.position, Fc.q1, Fc.q2, Fc.q3, Dco.constructorRef
FROM     FACTClasificaciones Fc join 
		 DIMPilotos Dp on Fc.driverId=Dp.driverId
         join DIMConstructores Dco on Fc.constructorId= Dco.constructorId 
		 join DIMCarreras Dca on Fc.raceId=Dca.raceId
         join DIMCircuitos Dci on Dca.circuitId=dci.circuitId
		 join DIMTemporadas Dt on Dca.year=Dt.year
where Dt.year=@Temporadas
and Dci.circuitId=@Circuito
--and Dca.raceId=@Carrera
order by position</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="position">
          <DataField>position</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="q1">
          <DataField>q1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="q2">
          <DataField>q2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="q3">
          <DataField>q3</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="driverRef">
          <DataField>driverRef</DataField>
